ublic owner;\\n    string public ipfsHash;\\n    bool public hashSet;\\n\\n    constructor() {\\n        owner = msg.sender;\\n    }\\n\\n    modifier onlyOwner() {\\n        require(msg.sender == owner, \\\"Only owner can perform this action\\\");\\n        _;\\n    }\\n\\n    function setIPFSHash(string memory _ipfsHash) public onlyOwner {\\n        ipfsHash = _ipfsHash;\\n        hashSet = true;\\n        emit HashAdded();\\n    }\\n\\n    function deleteIPFSHash() public onlyOwner {\\n        require(hashSet, \\\"IPFS hash not set\\\");\\n        ipfsHash = \\\"\\\";\\n        hashSet = false;\\n    }\\n\\n    function getIPFSHash() public view returns (string memory) {\\n        return ipfsHash;\\n    }\\n\\n    function postsToSendAvailable() public view returns (bool) {\\n        return hashSet;\\n    }\\n}\",\"keccak256\":\"0xaddb673ffe9897486b1fedb04e9cbc7174d65124211cbf41fd0bc2eacf1fe07f\",\"license\":\"UNLICENSED\"}},\"version\":1}",
